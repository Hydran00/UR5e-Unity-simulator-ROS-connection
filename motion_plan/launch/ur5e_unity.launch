 <launch>
 	<arg name="tcp_ip" default="0.0.0.0"/>
 	<arg name="tcp_port" default="10000"/>

 	<node name="server_endpoint" pkg="ros_tcp_endpoint" type="default_server_endpoint.py"  output="screen" respawn="true">
 		<param name="tcp_ip" type="string" value="$(arg tcp_ip)"/>
 		<param name="tcp_port" type="int" value="$(arg tcp_port)"/>
 	</node>
 	
 	 <node name="unity_ur5e_motion" pkg="motion_plan" type="Unity_UR5e.py" output="screen"></node>
 	<node name="unity_gripper_control" pkg="motion_plan" type="Unity_Gripper.py" output="screen"></node>
 </launch>
